# Load aliases from shell: source aliases
# Show aliases with: alias

alias ..='cd ..'
alias ...='cd ../..'

alias rmr='rm -r'

alias gitpull='git pull origin master'
alias gitpush='git push origin master'
alias gits='git status -s'
alias gita='git add'
alias gitc='git commit -m'
alias gitr='git rm'
alias gitrr='git rm -r'